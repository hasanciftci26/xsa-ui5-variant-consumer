COLUMN TABLE "CUSTOMERS"(
	CUSTOMER_ID NVARCHAR(15),
	COMPANY_NAME NVARCHAR(100),
	CONTACT_NAME NVARCHAR(100),
	CONTACT_TITLE NVARCHAR(50),
	ADDRESS NVARCHAR(255),
	CITY NVARCHAR(50),
	REGION NVARCHAR(50),
	POSTAL_CODE NVARCHAR(10),
	COUNTRY NVARCHAR(50),
	PHONE NVARCHAR(30),
	FAX NVARCHAR(30),
	PRIMARY KEY (CUSTOMER_ID)
)